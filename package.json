{
  "name": "wcc-frontend",
  "description": "Frontend NextJS application for WCC content/website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prepare": "husky",
    "build": "next build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "playwright test --grep-invert @visual",
    "test:e2e:run": "npx @playwright/test test",
    "test:e2e:docker": "docker compose run --rm --build playwright pnpm run test:e2e:run",
    "test:e2e:docker:update": "docker compose run --rm playwright pnpm run test:e2e:run --update-snapshots"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^5.15.18",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.15.18",
    "@testing-library/dom": "^10.1.0",
    "axios": "^1.7.2",
    "css-mediaquery": "^0.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "next": "14.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.66.0",
    "ts-jest": "^29.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/css-mediaquery": "^0.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "next-router-mock": "^0.9.13",
    "playwright": "~1.57.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
